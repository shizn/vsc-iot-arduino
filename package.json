{
    "name": "vsc-iot-arduino",
    "displayName": "vsc-iot-arduino",
    "description": "vsc-iot-arduino",
    "version": "0.0.1",
    "publisher": "yw",
    "engines": {
        "vscode": "^1.7.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "icon": "media/dep.png",
    "enableProposedApi": true,
    "contributes": {
        "explorer": {
            "treeLabel": "Arduino IoT Explorer",
            "icon": "media/dep.svg",
            "treeExplorerNodeProviderId": "sampleTree"
            },
        "languages": [{
            "id": "arduino",
            "aliases": ["Arduino", "arduino"],
            "extensions": [".pde",".ino"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "arduino",
            "scopeName": "source.c++.arduino",
            "path": "./syntaxes/Arduino.tmLanguage"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
